{
  "multipart": [
    {
      "apply": {
        "model": "refinedpipes:block/pipe"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 90
      }
    }
  ]
}