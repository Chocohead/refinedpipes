{
  "multipart": [
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension"
      }
    },
    {
      "when": {
        "north": false,
        "down": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_down"
      }
    },
    {
      "when": {
        "north": false,
        "up": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_up"
      }
    },
    {
      "when": {
        "north": false,
        "east": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_vertical_right"
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "y": 90
      }
    },
    {
      "when": {
        "east": false,
        "down": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_down",
        "y": 90
      }
    },
    {
      "when": {
        "east": false,
        "up": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_up",
        "y": 90
      }
    },
    {
      "when": {
        "east": false,
        "south": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_vertical_right",
        "x": 180
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 180
      }
    },
    {
      "when": {
        "south": false,
        "down": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_down",
        "x": 90
      }
    },
    {
      "when": {
        "south": false,
        "up": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_up",
        "y": 180
      }
    },
    {
      "when": {
        "south": false,
        "west": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_vertical_left",
        "x": 180
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior",
        "x": 180
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "y": 270
      }
    },
    {
      "when": {
        "west": false,
        "down": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_down",
        "y": 270
      }
    },
    {
      "when": {
        "west": false,
        "up": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_horizontal_up",
        "y": 270
      }
    },
    {
      "when": {
        "west": false,
        "north": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_frame_vertical_left"
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior",
        "y": 270
      }
    },
    {
      "when": {
        "up": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 270
      }
    },
    {
      "when": {
        "up": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior",
        "x": 270
      }
    },
    {
      "when": {
        "down": true
      },
      "apply": {
        "model": "refinedpipes:block/pipe_extension",
        "x": 90
      }
    },
    {
      "when": {
        "down": false
      },
      "apply": {
        "model": "refinedpipes:block/pipe_interior",
        "x": 90
      }
    },

    {
      "when": {
        "attachment_north": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment"
      }
    },
    {
      "when": {
        "attachment_east": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment",
        "y": 90
      }
    },
    {
      "when": {
        "attachment_south": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment",
        "x": 180
      }
    },
    {
      "when": {
        "attachment_west": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment",
        "y": 270
      }
    },
    {
      "when": {
        "attachment_up": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment",
        "x": 270
      }
    },
    {
      "when": {
        "attachment_down": "normal"
      },
      "apply": {
        "model": "refinedpipes:block/pipe_attachment",
        "x": 90
      }
    }
  ]
}